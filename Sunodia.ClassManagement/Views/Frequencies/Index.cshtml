@model IEnumerable<Sunodia.ClassManagement.Models.Frequency>

@{
    ViewBag.Title = "Index";
}

<h2>Course Frequencies</h2>


<p><a href="@Url.Action("ManageLists", "Home")" class="btn btn-primary btn-lg">Back &raquo;</a></p>
<p>
    @Html.ActionLink("Create New", "Create")

</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Frequency1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Frequency1)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
